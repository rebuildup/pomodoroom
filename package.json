{
  "name": "pomodoroom-desktop",
  "version": "1.4.17",
  "private": true,
  "description": "Pomodoroom Desktop App with Tauri",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
"scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "biome:check": "biome check src",
    "biome:format": "biome format src --write",
